    // Start phone Number Validation
    function phoneFormatter() {
        $('.numaric').on('input', function () {
            var number = $(this).val().replace(/[^.\d]/g, '')

            $(this).val(number);
            if (number.length == 16) {
                $(this).val(null);
                toastr.error('max limit 15 digit only');
                return false;
            }

        });
    };
    $(phoneFormatter());

    // End phone Number Validation

</script>
