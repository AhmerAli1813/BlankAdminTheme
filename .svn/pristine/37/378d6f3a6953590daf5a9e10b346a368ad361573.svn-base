
@{
    ViewBag.Title = "Container Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
  
    .badge {
    /*font-size: 12px;*/
    width: auto!important;
}
    .alertify .ajs-footer{
         display:none;
     }
    
        .ss-main .ss-multi-selected 
{
    min-height:38px!important;

}
.ss-add {
    /*margin: auto !important;*/
    padding-right: 12px;
    font:bold;
}


.ss-main .ss-multi-selected .ss-values .ss-value
{
    background-color:#455a64;
}
 .dtp div.dtp-date, .dtp div.dtp-time {
        background: #01213d !important;
    }
    .dtp .p10 > a {
        color: #ffffff !important;
    }
    .dtp table.dtp-picker-days tr > td > a.selected {
    background: #01213d !important;
    color: #fff;
}
    .dtp > .dtp-content > .dtp-date-view > header.dtp-header {
        background: #083358 !important;
    }
    .truncate{
        text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    width: 150px;
    }
   
</style>

<head>

    <link href="~/Content/BootsrapDatetime/bootstrap-material-datetimepicker.css" rel="stylesheet" />
    <link href="~/Content/BootsrapDatetime/css.css" rel="stylesheet" />
    <link href="~/Content/BootsrapDatetime/icon.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Content/BootsrapDatetime/ripples.min.js"></script>
    <script src="~/Content/BootsrapDatetime/material.min.js"></script>

    <script src="~/Scripts/js/plugins/moment/moment_locales.min.js"></script>
    <script src="~/Content/BootsrapDatetime/bootstrap-material-datetimepicker.js"></script>
  
    
    <link href="~/Scripts/SlimSelect/SlimSelectCustomStyle.css" rel="stylesheet" />
    <link href="~/Scripts/SlimSelect/slimselect.min.css" rel="stylesheet" />
    <script src="~/Scripts/SlimSelect/slimselect.min.js"></script>

    @*<script src="~/Scripts/DatePicker/jquery.min.js"></script>*@
    <link href="~/Scripts/DatePicker/bootstrap-datepicker.css" rel="stylesheet" />
    <link href="~/Scripts/DatePicker/bootstrap-datepicker.standalone.css" rel="stylesheet" />
    <script src="~/Scripts/DatePicker/bootstrap-datepicker.js"></script>
    <link href="~/Content/css/PaganationStyleSheetCustom.css" rel="stylesheet" />
    <!--Alertify CDN Start -->
    <link href="~/Scripts/AlertifyCdn/alertify.min.css" rel="stylesheet" />
    <script src="~/Scripts/AlertifyCdn/alertify.min.js"></script>
    <link href="~/Scripts/AlertifyCdn/default.min.css" rel="stylesheet" />
    <link href="~/Scripts/AlertifyCdn/jquery-confirm.min.css" rel="stylesheet" />



</head>

<div ng-controller="DWHCtrl" ng-init="CntrInit()" class="ng-cloak" ng-cloak>
    <div class="page-header border-bottom-0">
        <div class="page-header-content header-elements-md-inline">
            <div class="page-title d-flex">
                <h4>
                    <img src="~/Content/images/container.png"  class="onhover mr-2" width="25" title="Container Details">
                    <span class="font-weight-semibold">Containers Details</span><span class="ng-cloak" style="border-radius: 18px;margin-left: 12px;padding: 6px;color: white;width: 27px;background: #00946b;height: auto;" ng-cloak>{{resp.CntrCount}}</span>
                </h4>    
            </div>

            <div class="mt-3">
                    
                <input type="button" class="btn btn-secondary  pull-right" value="Filters" data-toggle="collapse" data-target="#demo" />
                  @if (User.IsInRole("Operations") || User.IsInRole("Admin"))
                  {
                    <input type="button" class="btn btn-secondary pull-right mr-2" ng-show="resp.CntrStatusIsClosed==false" value="Add Container" ng-click="ShowAddContainerModel()" />
                  }
                
                @*<input type="button" class="btn btn-secondary  pull-right mr-2" value="Vessel Details" data-toggle="collapse" data-target="#VesselDetails" />*@
            </div>
        </div>
    </div>
    <hr />

    <!--Search Filter Code Start-->
        <div class="card collapse"  id="demo">
            <div class="card-body">
                <div class="col-md-12  border-bottom">
                    <div class="row">

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Cntr</label>
                                <input type="text" id="TabCntr" class="form-control" placeholder="Cntr" ng-model="search.Cntr" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>BL Nbr</label>
                                <input type="text" class="form-control" id="Tabbl_nbr" placeholder="BL Nbr" ng-model="search.bl_nbr" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Category</label>
                                @*<input type="text" id="TabCategory" class="form-control" placeholder="Category" ng-model="search.category" />*@
                                <select id="searchcategory" class="is-valid" ng-model="search.category">
                                    <option data-placeholder="true"></option>
                                    <option ng-value="IMPRT">IMPRT</option>
                                    <option ng-value="EXPRT">EXPRT</option>
                                    <option ng-value="TRSHP">TRSHP</option>
                                    <option ng-value="THRGH">THRGH</option>
                                    <option ng-value="STRGE">STRGE</option>
                                </select>
                            </div>
                        </div>


                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Vgm(KG)</label>
                                <input type="text" class="form-control" id="Tabgoods_and_ctr_wt_kg" placeholder="Vgm(KG)" ng-model="search.goods_ctr_wt_kg_vgm" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Line Operator</label>
                                <input type="text" class="form-control" id="Taboprname" placeholder="Line Operator" ng-model="search.opr" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Vessel Operator</label>
                                <input type="text" class="form-control" id="TabLineOprName" placeholder="Vessel Operator" ng-model="search.LineOpr" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Commodity</label>
                                <input type="text" class="form-control" id="Tabcommodity" placeholder="Commodity" ng-model="search.commodity" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Destination</label>
                                <input type="text" class="form-control" id="Tabdestination" placeholder="Destination" ng-model="search.destination" />
                            </div>
                        </div>
                    
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Origin</label>
                                <input type="text" class="form-control" id="TabOrigin" placeholder="Origin" ng-model="search.Origin" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>InspStages</label>
                                <input type="text" class="form-control" id="TabInspStages" placeholder="InspStages" ng-model="search.InspStages" />
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Move Kind</label>
                                <input type="text" class="form-control" id="TabMoveKind" placeholder="Move Kind" ng-model="search.move_kind" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Nominal Length</label>
                                @*<input type="text" class="form-control" id="TabNominalLength" placeholder="Nominal Length" ng-model="search.nominal_length" />*@
                                <select id="searchnominal_length" class="is-valid" ng-model="search.nominal_length">
                                    <option data-placeholder="true"></option>
                                    <option ng-value="NOM20">NOM20</option>
                                    <option ng-value="NOM40">NOM40</option>
                                </select>
                             </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Nominal Height</label>
                                @*<input type="text" class="form-control" id="TabnominalHeight" placeholder="Nominal Height" ng-model="search.nominal_height" />*@
                                <select id="searchnominal_height" class="is-valid" ng-model="search.nominal_height">
                                    <option data-placeholder="true"></option>
                                    <option ng-value="NOM86">NOM86</option>
                                    <option ng-value="NOM96">NOM96</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>ISO</label>
                                <input type="text" class="form-control" id="TabISO" placeholder="ISO" ng-model="search.ISO" />
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>AEO</label>
                                <input type="text" class="form-control" id="TabAEO" placeholder="AEO" ng-model="search.AEO" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Is oog</label>
                                <select id="TabisOog" class="input displaySelect" ng-model="search.is_oog">
                                    <option data-placeholder="true"></option>
                                    <option ng-value="true">Yes</option>
                                    <option ng-value="false">NO</option>
                                </select>



                                @*<input type="text" class="form-control" id="TabisOog" placeholder="Is oog" ng-model="search.is_oog" />*@
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Is Bundle</label>
                                <select id="TabisBundle" class="input displaySelect" ng-model="search.is_bundle">
                                    <option data-placeholder="true"></option>
                                    <option ng-value="true">Yes</option>
                                    <option ng-value="false">NO</option>
                                </select>

                                @*<input type="text" class="form-control" id="TabisBundle" placeholder="Is Bundle" ng-model="search.is_bundle" />*@
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Custom Insp Type</label>
                                <input type="text" class="form-control" id="TabCustomInspType" placeholder="Custom Insp Type" ng-model="search.CustomInspType" />
                            </div>
                        </div>
                      
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Is Transit</label>
                                <input type="text" class="form-control" id="TabIsTransit" placeholder="Is Transit" ng-model="search.IsTransit" />
                            </div>
                        </div>
                           <div class="col-md-2">
                               <div class="form-group">
                                    <label>ITTTo</label>
                                        <input type="text" class="form-control" id="TabIsTransit" placeholder="ITTTo" ng-model="search.ITTTo" />
                                  </div>
                              </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>XrayStatus</label>
                                <input type="text" class="form-control" id="TabXrayStatus" placeholder="XrayStatus" ng-model="search.XrayStatus" />
                            </div>
                        </div>
                        @*<div class="col-md-3">
                    <div class="form-group">
                        <label>From</label>
                        @*<input type="date" class="form-control" placeholder="From" ng-model="data.FromDate" />
                        <div id="sandbox-container">
                            <div class="input-group date">
                                <input type="text" class="form-control" id="TabFromDate" placeholder="dd/mm/yyyy"><span class="input-group-addon"><i class="mi-insert-invitation"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>To</label>
                        <input type="date" class="form-control" placeholder="To" ng-model="data.ToDate" />
                        <div id="sandbox-container">
                            <div class="input-group date">
                                <input type="text" class="form-control" id="TabToDate" placeholder="dd/mm/yyyy"><span class="input-group-addon"><i class="mi-insert-invitation"></i></span>
                            </div>
                        </div>
                    </div>
                </div>*@
                        <div class="col-md-3">
                            <div class="form-group">
                                <button type="button" class="btn btn-theme-gray pull-right mt-3"  ng-click="ResetCntr()" >Reset</button>
                                @*<input type="button" class="btn btn-primary pull-right  mr-2 mt-3" value="Search"  />*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <!--Search Filter Code End-->
    <!--Vessel Master Information Start-->
    <div class="card ng-cloak"  ng-cloak>
        <div class="card-body">
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label><b>Reference No</b></label>
                        <div>{{resp.RefNo}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-3" ng-show="resp.VesselName!=null && resp.VesselName!=''">
                    <div class="form-group">
                        <label><b>Vessel Name</b></label>
                        <div>{{resp.VesselName}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-3">
                    <div class="form-group">
                        <label><b>ATD</b></label>
                        <div>{{resp.atd |date:'dd-MM-yyyy HH:mm:ss'}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-2" ng-show="resp.AgentName!=null && resp.AgentName!=''">
                    <div class="form-group">
                        <label><b>Agent Name</b></label>
                        <div>{{resp.AgentName}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-2" ng-show="resp.LineOprName!=null && resp.LineOprName!=''">
                    <div class="form-group">
                        <label><b>Line Operator </b></label>
                        <div>{{resp.LineOprName}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-2" ng-show="resp.SailDirection!=null && resp.SailDirection!=''">
                    <div class="form-group">
                        <label><b>SailDirection</b></label>
                        <div>{{resp.SailDirection}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-3" ng-show="resp.VSClassName!=null && resp.VSClassName!=''">
                    <div class="form-group">
                        <label><b>VS Class Name</b></label>
                        <div>{{resp.VSClassName}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-3" ng-show="resp.SrvName!=null && resp.SrvName!=''">
                    <div class="form-group">
                        <label><b>SrvName</b></label>
                        <div>{{resp.SrvName}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>

                <div class="col-md-2">
                    <div class="form-group">
                        <label><b>Vessel Status</b></label>
                        <div>
                            <span class="badge badge-danger" ng-show="resp.VesselStatusCode=='invalid'">{{resp.VesselStatus}}</span>
                            <span class="badge badge-success" ng-show="resp.VesselStatusCode=='valid'">{{resp.VesselStatus}}</span>
                            <span class="badge badge-warning" ng-show="resp.VesselStatusCode=='cntrO'">{{resp.VesselStatus}}</span>
                            <span class="badge badge-info" ng-show="resp.VesselStatusCode=='cntrF'">{{resp.VesselStatus}}</span>
                        </div>
                    </div>
                    @*<div class="dropdown-divider"></div>*@
                </div>
                <div class="col-md-2" ng-show="resp.UpdatedAt!=null && resp.UpdatedAt!=''">
                    <div class="form-group">
                        <label><b>Updated At</b></label>
                        <div>{{resp.UpdatedAt | date:'dd-MM-yyyy'}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>
                <div class="col-md-2" ng-show="resp.UpdatedBy!=null && resp.UpdatedBy!=''">
                    <div class="form-group">
                        <label><b>Updated By</b></label>
                        <div>{{resp.UpdatedBy}}</div>
                    </div>
                    <div class="dropdown-divider"></div>
                </div>
                <div class="col-md-3" ng-show="resp.Remarks!=null && resp.Remarks!=''">
                    <div class="form-group">
                        <label><b>Remarks</b></label>
                        <div>
                         <textarea class="form-control"rows="4" disabled  cols="12" ng-model="resp.Remarks" placeholder="Remarks"></textarea>
                        </div>
                    </div>
                    @*<div class="dropdown-divider"></div>*@
                </div>

            </div>
        </div>
    </div>
    <!--Vessel Master Information End-->
    <div class="row">
      <div class="col-md-12">
              <button ng-click="ExpotToExcelContainer(resp.RefNo ,search)" class="btn btn-secondary pull-right mr-2 ">Export To Excel</button> 
              @if (User.IsInRole("Finance") || User.IsInRole("Admin"))
              {
                <a class="btn btn-secondary cursor-pointer text-white mr-2 pull-right" ng-show="resp.CntrStatusIsClosed==false" ng-click="UpdateContainerStatusMethod()">Validate & Close</a>
              }

                @if (User.IsInRole("Operations") || User.IsInRole("Admin"))
                {
                    <a class="btn btn-primary cursor-pointer text-white pull-right mr-2" ng-show="resp.VesselStatusCode=='cntrO' && resp.CntrStatusIsClosed==false" ng-click="ShowUpdateStatusModal()" ng-cloak>Finance Confirmation</a>
                }
             
              @if (User.IsInRole("Finance") || User.IsInRole("Admin"))
              {
                <a class="btn btn-primary cursor-pointer text-white pull-right mr-2" ng-show="resp.VesselStatusCode!='cntrO' && resp.CntrStatusIsClosed==false"  ng-click="ShowUpdateStatusModal()" ng-cloak>Operation Review</a>
              }
      </div>
    </div>
    <div class="row">

        <div id="tblrep" class="card table-card scrollbar">
           
            <div class="row">
                <div class="col-md-12">
                    <table class="table datatable-show-all text-center" id="table-id">
                        <thead>
                            <tr class="table-card-head">
                                <th>SR#</th>
                                <th>Cntr</th>
                                <th>Category</th>
                                <th>Vgm(KG)</th>
                                <th>Line Operator  @*opr*@</th>
                                <th>Vessel Operator </th>
                                <th>Commodity</th>
                                <th>Destination</th>
                                <th>BL Nbr</th>
                                <th>Origin</th>
                                <th>InspStages</th>

                                <th>Move Kind</th>
                                <th>Nominal Length</th>
                                <th>Nominal Height</th>
                                <th>AEO</th>
                                <th>Is oog</th>
                                <th>Is Bundle</th>
                                <th>Custom Insp Type</th>
                                <th>ISO</th>
                                <th>Is Transit</th>
                                <th>ITTTo</th>
                                <th>Xray Status</th>
                                <th>Status</th>
                                <th style="width:100px;">Action</th>

                            </tr>


                        </thead>
                        <tbody>
                            @*class="clickable-style"*@ @*ng-click="GetTask(item.RefNo)"*@
                            <tr class="clickable-style" ng-repeat="item in VesselContainersDetails | filter:search">
                                <td>{{$index+1}}</td>
                                <td>{{item.Cntr}}</td>
                                <td>{{item.category}}</td>
                                <td>{{item.goods_ctr_wt_kg_vgm}}</td>
                                <td>{{item.opr}}</td>
                                <td>{{item.LineOpr}}</td>
                                <td>
                                    <div class="truncate">{{item.commodity}}</div>
                                </td>
                                <td>{{item.destination}}</td>
                                <td>{{item.bl_nbr}}</td>
                                <td>{{item.Origin}}</td>
                                <td>{{item.InspStages}}</td>
                                <td>{{item.move_kind}}</td>
                                <td>{{item.nominal_length}}</td>
                                <td>{{item.nominal_height}}</td>
                                <td>{{item.AEO}}</td>
                                <td>
                                    <span ng-if="item.is_oog == true">Yes</span>

                                    <span ng-if="item.is_oog == false">No</span>
                                </td>
                                <td>
                                    <span ng-if="item.is_bundle == true">Yes</span>

                                    <span ng-if="item.is_bundle == false">No</span>
                                </td>
                                <td>{{item.CustomInspType}}</td>
                                <td>{{item.ISO}}</td>
                                <td>{{item.IsTransit}}</td>
                                <td>{{item.ITTTo}}</td>
                                <td>{{item.XrayStatus}}</td>

                                <td>
                                    <span class="badge badge-danger" ng-show="item.StatusCode=='invalid'">{{item.Status}}</span>
                                    <span class="badge badge-success" style="font-size: 15px;" ng-show="item.StatusCode=='valid'">{{item.Status}}</span>
                                    <span class="badge badge-secondary" style="font-size: 15px;" ng-show="item.StatusCode=='cntrClosed'">{{item.Status}}</span>
                                 </td>
                                <td style="width:100px">
                                    <a ng-click="ContainerPrint(item.Cntr,item.RefNo)">
                                        <i class="icon-printer" title="Print"></i>
                                    </a>
                                    <a ng-click="DWHContainerPdf(item.Cntr,item.RefNo)"><i class="icon-file-pdf" title="PDF"></i></a>

                                    <a>
                                        <i class="icon-file-excel cursor-pointer" title="Export To Excel" ng-click="CntrExcel(item.Cntr,item.RefNo)"></i>
                                    </a>

                                    @if (User.IsInRole("Admin") || User.IsInRole("Operations"))
                                    {
                                        <a ng-show="(item.StatusCode=='invalid' || resp.VesselStatusCode=='cntrO') &&(resp.VesselStatusCode!='valid' && resp.CntrStatusIsClosed ==false)">
                                            <i class="icon-pencil7 cursor-pointer" title="Edit Container Details" ng-click="ShowContainerDetailsEditModel(item)"></i>
                                        </a>
                                    }

                                   
                                    <a ng-show="item.StatusCode=='valid' ||( _currentUser.UserType.includes('Finance') || _currentUser.UserType.includes('Admin'))">
                                        <i class="icon-eye cursor-pointer" style="font-size: large; /*cursor: not-allowed*/" ng-click="ShowContainerDetailsReadOnlyModel(item)" title="View"></i>
                                    </a>
                                    @if (User.IsInRole("Admin") || User.IsInRole("Operations"))
                                    {
                                        <a>
                                            <i class="icon-bin2 cursor-pointer" data-toggle="modal" data-target="#myModal" ng-show="resp.VesselStatusCode!='valid' && resp.CntrStatusIsClosed ==false" title="Delete Container" ng-click="DeleteRecord(item.Id,item,'Cntr')"></i>
                                        </a>
                                    }
                                  

                                </td>
                            </tr>

                        </tbody>
                    </table>
                    <div class="col-md-12">
                        <button class="floatBottom btn btn-primary" style="width: -webkit-fill-available;" ng-click="LoadMore('Cntr')" ng-show="floatButton">Load More</button>
                    </div>
                </div>
              
            </div>
           


        </div>

        <!--Alertify Modal Container Edit Start-->
        <form autocomplete="off" style="display:none;width: auto;position: relative;margin: 0px auto;" id="ContainerDetailsEdit" name="ContainerDetailsEdit">

            <div class="form-body" style="margin:10px;margin-top: 20px;">
                <div class="row p-t-20">

                    <div class="col-md-2">
                        <div class="form-group">
                            <label><b>Referenece No</b></label>
                            <div>{{resp.RefNo}} </div>
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="form-group">
                            <label><b>Cntr No</b></label>
                            <div>{{CntrDeatils.Cntr}} </div>
                        </div>

                    </div>

                    <div class="col-md-2">
                        <div class="form-group">
                            <label><b>Status</b></label>
