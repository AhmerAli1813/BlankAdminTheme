<div ng-controller="DWHCtrl">


    <div class="row ng-cloak" style="margin-top: 13px;" ng-cloak>
        <div class="col-md-12">
            <!--Tab Code Start -->
            <div class="tab">
                <div class="row">
                    <div class="col-md-3 text-center ">
                        <button class="tablinks active" onclick="openCity(event, 'tab1')" ng-init="init()"><i class="icon-task mr-2"></i> Awaiting Actions</button>
                    </div>
                    <div class="col-md-3 text-center ">
                        <button class="tablinks" onclick="openCity(event, 'tab2');"><i class="icon-file-check mr-2"></i>Validate Vessels Sailed</button>
                    </div>


                    <div class="col-md-6 text-center">
                        <div class="text-center timeHeader pull-right">
                            <span>
                                @DateTime.Now.ToString("dd-MM-yyyy")
                            </span>
                            <span ng-controller="JIPCtrl">
                                {{theTime}}
                            </span>
                        </div>
                    </div>


                </div>
            </div>
            <!--Tab Code End -->
        </div>
    </div>
    <div class="card">
        <div class="card-body" style="padding:0px!important">

            <!--Awating Action Tab Content Tab 1 start -->
            <div id="tab1" class="tabcontent ng-cloak" style="display:block" ng-cloak>
                <!--NG-controller Class Awating Action Start-->
                <div>
                    <div class="page-header border-bottom-0">
                        <div class="page-header-content header-elements-md-inline">
                            <div class="page-title d-flex">
                                <h4>
                                    <i class="icon-task mr-2"></i>
                                    <span class="font-weight-semibold">Awaiting Actions</span>
                                </h4>

                            </div>

                            <div class="mt-3">



                            </div>
                            <div class="mt-3">
                                <input type="button" class="btn btn-secondary  pull-right" value="Filters" data-toggle="collapse" data-target="#demo" />

                                <button ng-click="ExportToExcel1('1',search1)" class="btn btn-secondary mr-2 ml-5">Export To Excel</button>

                            </div>
                        </div>
                    </div>
                    <hr />
                    <!-- /page header -->
                    <!--Search Filter Code Start-->
                    <div class="card collapse" id="demo">
                        <div class="card-body">
                            <div class="col-md-12">
                                <div class="row">

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Reference No</label>
                                            <input type="text" id="TabRefNo" class="form-control" placeholder="Reference No" ng-model="search1.RefNo" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Vessel Name</label>
                                            <input type="text" id="TabVesselName" class="form-control" placeholder="Vessel Name" ng-model="search1.VesselsName" />
                                        </div>
                                    </div>


                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Agent Name</label>
                                            <input type="text" class="form-control" id="TabAgent" placeholder="Agent Name" ng-model="search1.Agent" />
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Hatch Cover</label>
                                            <input type="text" class="form-control" id="TabHatchCover" placeholder="Hatch Cover" ng-model="search1.HatchCover" />
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Line Operator </label>
                                            <input type="text" class="form-control" id="TabLineOperatorName" placeholder="Line Operator " ng-model="search1.LineOperatorName" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Sail Direction</label>
                                            <input type="text" class="form-control" id="TabSailDirection" placeholder="Sail Direction" ng-model="search1.SailDirection" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Vessel Class Name</label>
                                            <input type="text" class="form-control" id="TabVS_Class_Name" placeholder="VS Class Name" ng-model="search1.VS_Class_Name" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Srv Name</label>
                                            <input type="text" class="form-control" id="TabSrvName" placeholder="Srv Name" ng-model="search1.SrvName" />
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>From</label>
                                            @*<input type="date" class="form-control" placeholder="From" ng-model="data.FromDate" />*@
                                            <div id="sandbox-container">
                                                <div class="input-group date">
                                                    <input type="text" class="form-control" id="TabFromDate" placeholder="dd/mm/yyyy"><span class="input-group-addon"><i class="mi-insert-invitation"></i></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>To</label>
                                            @*<input type="date" class="form-control" placeholder="To" ng-model="data.ToDate" />*@
                                            <div id="sandbox-container">
                                                <div class="input-group date">
                                                    <input type="text" class="form-control" id="TabToDate" placeholder="dd/mm/yyyy"><span class="input-group-addon"><i class="mi-insert-invitation"></i></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group mt-1">
                                            <input type="button" class="btn btn-theme-gray pull-right mt-3" value="Reset" ng-click="Reset('Invalid')">
                                            <input type="button" class="btn btn-primary pull-right  mr-2 mt-3" value="Search" ng-click="VistIndexSearch(1)">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                  
                    <!--Search Filter Code End-->
                    <!--Table-->
                    <div class="row">

                        <div id="tblrep" class="card table-card scrollbar">
                            <table class="table datatable-show-all text-center" id="table-id">
                                <thead>
                                    <tr class="table-card-head">
                                        <th>SR#</th>
                                        <th>Reference No</th>
                                        <th>Vessel Name</th>
                                        <th>Agent Name</th>
                                        <th>Hatch Cover</th>
                                        <th>@*Line*@Vessel Operator</th>
                                        <th>SailDirection</th>
                                        <th>Vessel Class Name</th>
                                        <th>ATD</th>
                                        <th>SrvName</th>
                                        <th style="width:10%">Deatils</th>
                                        <th style="width:8%">Action</th>
                                        <th>Status</th>
                                        <th>Remarks</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @*class="clickable-style"*@ @*ng-click="GetTask(item.RefNo)"*@
                                    <tr ng-repeat="item in VesselSailedDetails | filter:search1">
                                        <td>{{$index+1}}</td>
                                        <td>{{item.RefNo}}</td>
                                        <td>{{item.VesselsName}}</td>
                                        <td>{{item.Agent}}</td>
                                        <td>{{item.HatchCover}}</td>
                                        <td>{{item.LineOperatorName}}</td>
                                        <td>{{item.SailDirection}}</td>
                                        <td>{{item.VS_Class_Name}}</td>
                                        <td>{{item.atd | date:'dd-MM-yyyy HH:mm:ss'}}</td>
                                        <td>{{item.SrvName}}</td>
                                        <td>
                                            <a>
                                                <!--Crane-->
                                                <img src="~/Content/images/crane2.png" id="{{$index+1}}" ng-click="ShowCraneDetailsModel(item)" ng-mouseover="changeicon($index+1,'move','crane')" ng-mouseleave="changeicon($index+1,'leave','crane')" class="onhover list-icons-item" width="25" title="Crane Details" />
                                            </a>
                                            <a>
                                                <i class="icon-ship onhover list-icons-item" title="Vessel Details" style="font-size:x-large" ng-click="ShowAwatingForm(item)"></i>
                                            </a>
                                            <a @*href="DWH/CntrIndex?RefNo={{item.RefNo}}" target="_blank"*@ ng-click="Container(item.RefNo)">
                                                <!--Container-->
                                                <img src="~/Content/images/container.png" id="{{$index+1+'a'}}" ng-mouseover="changeicon($index+1+'a','move','container')" ng-mouseleave="changeicon($index+1+'a','leave','container')" class="onhover list-icons-item" width="25" title="Container Details" />
                                            </a>
                                       
                                          
                                         
                                        </td>

                                        <td>
                                         
                                            <a ng-click="DWHVesselPrint(item.Id)" @*href="Reports/DWHVesselPrint" target="_blank"*@>
                                                <i class="icon-printer" title="Print"></i>
                                            </a>
                                            <a ng-click="DWHVesselPdf(item.Id)"><i class="icon-file-pdf" title="PDF"></i></a>

                                            <a ng-click="VesselExportToExcel(item.Id,1)"><i class="icon-file-excel" title="Export To Excel"></i></a>
                                        </td>
                                        <td>
                                            <span class="badge badge-danger" ng-show="item.StatusCode=='invalid'">{{item.Status}}</span>
                                            <span class="badge badge-warning" ng-show="item.StatusCode=='cntrO'">{{item.Status}}</span>
                                            <span class="badge badge-info" ng-show="item.StatusCode=='cntrF'">{{item.Status}}</span>
                                        </td>
                                        <td>
                                           <div class="truncate">
                                               {{item.Remarks}}
                                           </div>
                                        </td>
                                    </tr>

                                </tbody>
                            </table>
                            <button ng-click="LoadMore('Invalid')" ng-show="floatButton" class="floatBottom btn btn-primary pull-right">Load More</button>

                        </div>
