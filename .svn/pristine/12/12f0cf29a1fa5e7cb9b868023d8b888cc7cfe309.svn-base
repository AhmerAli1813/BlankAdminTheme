@using DPWVessel.Web.Core.Session
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>

    .floatBottom {
        margin-top: 10px;
    }
    .timeHeader {
        background: #00946b;
        padding: 15px;
        color: white;
        font-size: 17px;
        border-top-right-radius: 10px;
        border-top-left-radius: 10px;
    }
    .tab {
        overflow: hidden;
        /*border: 1px solid #ccc;
        background-color: #f1f1f1;*/
    }
    .tabheading {
        padding: 10px;
        background: #00946b;
        color: white;
    }
    /* Style the buttons inside the tab */
    .tab button {
        border-top-right-radius: 10px;
        border-top-left-radius: 10px;
        background: #e9e9e9;
        padding: 15px;
        /*float: left;*/
        border: none;
        outline: none;
        cursor: pointer;
        /*padding: 14px 16px;*/
        transition: 0.3s;
        font-size: 17px;
    }
        /* Change background color of buttons on hover */
        .tab button:hover {
            background-color: #00946b;
            color: white;
            font-size: 17px;
        }

        /* Create an active/current tablink class */
        .tab button.active {
            background-color: #00946b;
            color: white;
            font-size: 17px;
        }

    .tab button {
        width: 100%;
    }

    /* Style the tab content */
    .tabcontent {
        display: none;
        padding: 0px 10px;
        /*border: 1px solid #ccc;*/
        border-top: none;
    }

    .badgeRequired {
        /*position: fixed!important;*/
    }

        /*.truncate {
            width: 99px;
            word-break: break-all;
        }*/
     .truncate{
        text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    width: 90px;
    }
    label, tbody, .btn {
        font-weight: 500 !important;
    }

    .badge {
        /*font-size: 12px;*/
        width: auto !important;
    }

    .alertify .ajs-footer {
        display: none;
    }

    a {
        cursor: pointer !important;
    }
</style>

