
@{
    ViewBag.Title = "Dashboard";
}

<div class="mt-30">
    <div class="section">
        <h4>Page Title Heading and Button</h4>
        <!--for Page Title-->
        <div class="page-title">
            <div class="flex">
                <div class="Flex-coumn"><h4><i class="fas fa-file-alt"></i>  Forms</h4></div>
                <div class="Flex-coumn">
                    <button type="button" class="Customebtn btnSuccess  pull-right mr-10"><span>Filters</span></button>
                </div>
            </div>
            <hr />
        </div>
        <!--End-->
    </div>

    <div class="section">
        <h4>Collapseable Filter</h4>
        <div class="page-title">
            <!--Filter Collapse-->
            <button type="button" class="Customebtn btnSuccess  mr-10"
                    data-bs-toggle="collapse"
                    data-bs-target="#Filters"
                    aria-expanded="false"
                    aria-controls="Filters">
                <span>Filters</span>
            </button>
            <div class="card collapse" id="Filters">
                <div class="card-header">
                    Filters
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>User Id</label>
                                <input type="text" class="form-control" placeholder="Id" ng-model="search.Id" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Name</label>
                                <input type="text" class="form-control" placeholder="Name" ng-model="search.FullName" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Email</label>
                                <input type="text" class="form-control" placeholder="Email" ng-model="search.Email" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Login Id</label>
                                <input type="text" class="form-control" placeholder="Login Id" ng-model="search.LoginId" />
                            </div>
                        </div>

                        <div class="col-md-12 float-right">
                            <div class="form-group">
                                <button type="button" onclick="reload()" class="Customebtn btnSecondary mr-10 pull-right mt-20"><span>Reset</span></button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--Filter Collapse End-->
        </div>
        <!--End-->
    </div>

    <div class="section">
        <h4>Page Title with multi item</h4>
        <!--for Page Title-->
        <div class="page-title">
            <div class="MultiFlex">
                <div class="MultiFlex-coumn">
                    <h4><i class="fas fa-file-alt"></i>  Forms</h4>
                    <span class="badge bg-danger">Danger</span>
                </div>
                <div class="MultiFlex-coumn-Select">
                    <div class="form-group">
                        <select class="form-select" aria-label="Default select example">
                            <option selected>Open this select menu</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>
                    <button type="button" class="Customebtn btnSuccess  pull-right mr-10"><span>Print</span></button>
                    <button type="button" class="Customebtn btnPrimary  pull-right mr-10"><span>Update</span></button>
                </div>


            </div>
            <hr />
        </div>
        <!--End-->
    </div>

    <div class="section">
        <h4>Buttons</h4>
        <!--for Collapse Content-->
        <button class="Customebtn btnPrimary mr-10"><span>Submit</span></button>
        <button class="Customebtn btnSecondary mr-10"><span>Cancel</span></button>
        <button class="Customebtn btnSuccess mr-10"><span>Print</span></button>
        <button class="Customebtn btnPrimary mr-10"><span>Save</span></button>
        <button class="Customebtn btnSuccess mr-10"><span>Export To Excel</span></button>
        <button class="Customebtn btnDanger mr-10"><span>Delete</span></button>
        <button type="button" class="btn btn-secondary" data-bs-toggle="tooltip" data-bs-placement="top" title="Tooltip on top">
            Tooltip on top
        </button>
        <!--End-->
    </div>
    <div class="section">
        <h4>Card</h4>
        <!--for Card Content-->
        <div class="card">
            <div class="card-header">
                Card Heading
            </div>
            <div class="card-body">
                Content
            </div>
        </div>
        <!--for Card Content-->
    </div>
    <div class="section">
        <h4>Forms Fields</h4>
        <!--for Forms Fileds Title-->
        <div class="container">
            <form class="needs-validation" novalidate">
                <div class="row">
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group" data-provide="datepicker">
                            <label for="exampleFormControlInput1" class="form-label Starick">Email address</label>
                            <input type="datetime-local" class="form-control is-invalid" id="exampleFormControlInput1" placeholder="name@example.com">
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group">
                            <label class="form-label">Status</label>
                            <select class="form-select" aria-label="Default select example">
                                <option selected>Open this select menu</option>
                                <option value="1">One</option>
                                <option value="2">Two</option>
                                <option value="3">Three</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group">
                            <label class="form-label">Default file input example</label>
                            <input class="form-control is-valid" type="file">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group">
                            <label class="form-label">Default example</label>
                            <div class="input-group mb-3">
                                <input type="text" class="form-control" aria-label="Text input with dropdown button">
                                <select class="form-select" aria-label="Default select example">
                                    <option selected>Open this select menu</option>
                                    <option value="1">One</option>
                                    <option value="2">Two</option>
                                    <option value="3">Three</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group">
                            <label class="form-label">Default example</label>
                            <select class="select2-multi"
                                    name="states[]" multiple="multiple"
                                    data-tags="true"
                                    data-placeholder="Select an option"
                                    data-allow-clear="true">
                                <option value="AL">Alabama</option>
                                <option value="WY">Wyoming</option>
                                <option value="AL">Alabama</option>
                                <option value="WY">Wyoming</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <label for="validationCustomUsername" class="form-label">Username</label>
                        <div class="input-group has-validation">
                            <span class="input-group-text" id="inputGroupPrepend"><i class="fas fas-user"></i></span>
                            <input type="text" class="form-control" id="validationCustomUsername" aria-describedby="inputGroupPrepend" required>
                            <div class="invalid-feedback">
                                Please choose a username.
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group">
                            <label class="form-label">Default example</label>
                            <select class="select2-single" name="state">
                                <option value="AL">Alabama</option>
                                ...
                                <option value="WY">Wyoming</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group">
                            <label class="form-label">Default example</label>
                            <select class="slims">
                                <optgroup label="Label 1">
                                    <option value="value 1">Value 1</option>
                                    <option value="value 2">Value 2</option>
                                    <option value="value 3">Value 3</option>
                                </optgroup>
                                <optgroup label="Label 2">
                                    <option value="value 21">Value 1</option>
                                    <option value="value 22">Value 2</option>
                                    <option value="value 23">Value 3</option>
                                </optgroup>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group">
                            <div class="mb-3">
                                <label for="exampleFormControlTextarea1" class="form-label">Example textarea</label>
                                <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group mt-38">
                            <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                            <label class="form-check-label" for="flexCheckDefault">
                                Default checkbox
                            </label>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="form-group mt-38">
                            <input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault1">
                            <label class="form-check-label" for="flexRadioDefault1">
                                Default radio
                            </label>
                        </div>
                    </div>

                    <details>
                        <summary>Epcot Center</summary>
                        <p>Epcot is a theme park at Walt Disney World Resort featuring exciting attractions, international pavilions, award-winning fireworks and seasonal special events.</p>
                    </details>


                </div>
            </form>
        </div>
        <!--End-->
    </div>
    <div class="section">
        <h4>Bootstrap Icon</h4>
        <!--for Bootstrap Icon Title-->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
        <i class="bi bi-check-circle-fill"></i>
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-justify" viewBox="0 0 16 16">
